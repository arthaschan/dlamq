ALTER TABLE CDM.T_CDM_DL_FLIGHT
 DROP PRIMARY KEY CASCADE;

DROP TABLE CDM.T_CDM_DL_FLIGHT CASCADE CONSTRAINTS;

CREATE TABLE CDM.T_CDM_DL_FLIGHT
(
  FID             VARCHAR2(36 BYTE)             NOT NULL,
  FUUID           VARCHAR2(36 BYTE),
  FINOUT          VARCHAR2(5 BYTE),
  FFLIGHTNUMBER   VARCHAR2(10 BYTE),
  FFLIGHTDATE     DATE,
  FAIRLINE        VARCHAR2(3 BYTE),
  FPOK            DATE,
  FBOR            DATE,
  FBLOCKDOWNTIME  DATE,
  FBLOCKUPTIME    DATE,
  FSEAT           VARCHAR2(10 BYTE)
)
TABLESPACE TBS_CDM
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
NOPARALLEL
MONITORING;

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FID IS '开始登机';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FUUID IS 'T_CDM_FLIGHTINFO里的ID
';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FINOUT IS 'IN:进港
OUT:出港
';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FFLIGHTNUMBER IS '航班号';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FFLIGHTDATE IS '航班日期
';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FAIRLINE IS '航空公司 3字码
';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FPOK IS '结束登机';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FBLOCKDOWNTIME IS '撤轮档';

COMMENT ON COLUMN CDM.T_CDM_DL_FLIGHT.FBLOCKUPTIME IS '上轮档';



CREATE UNIQUE INDEX CDM.T_CDM_DL_FLIGHT_PK ON CDM.T_CDM_DL_FLIGHT
(FID)
LOGGING
TABLESPACE TBS_CDM
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
NOPARALLEL;


ALTER TABLE CDM.T_CDM_DL_FLIGHT ADD (
  CONSTRAINT T_CDM_DL_FLIGHT_PK
  PRIMARY KEY
  (FID)
  USING INDEX CDM.T_CDM_DL_FLIGHT_PK);
